  "rich-text#title-second-section-how-buy": {
    "title": "Título",
    "props": {
      "blockClass": "how-buy-content__title",

      "text": "### Acesse sua Conta",
      "textPosition": "CENTER"
    }
  },
  "rich-text#content-second-section-how-buy": {
    "title": "Conteúdo",
    "props": {
      "blockClass": "how-buy-content__data",
      "text": "Copie o **Código de Primeiro Acesso** enviado para o seu email e cole no campo determinado na área de Login e clique em Entrar. Você será redirecionado para a Home automaticamente.",
      "textPosition": "CENTER"
    }
  },

  "flex-layout.row#main-container-third-section-how-buy": {
    "title": "Terceira Sessão",
